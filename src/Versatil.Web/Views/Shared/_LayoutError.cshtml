<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - AdminCaixa</title>

    <link rel="shortcut icon" href="~/img/favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/img/favicon.ico" type="image/x-icon">

    <link href="~/bundles/styles-plugins.bundle.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/bundles/styles-theme.bundle.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/bundles/styles-global.bundle.min.css" rel="stylesheet" asp-append-version="true">

    @RenderSection("Styles", required: false)
</head>
<body>
    @RenderBody()

    <script src="~/bundles/scripts-plugins.bundle.min.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
